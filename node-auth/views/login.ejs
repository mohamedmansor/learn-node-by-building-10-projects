<html lang="en">

<head>

    <%- include layout.ejs %>
        <title>
            <%= title %>
        </title>
</head>

<body>
    <h1>Login</h1>
    <p>Please login below.</p>
    <ul class="errors">
    </ul>
    <form method="post" action="/users/login" enctype="multipart/form-data">
        <div class="form-group">
            <label>Username</label>
            <input name="username" type="text" placeholder="Enter Username" class="form-control" />
        </div>
        <div class="form-group">
            <label>Password</label>
            <input name="password" type="password" placeholder="Enter Password" class="form-control" />
        </div>
        <input name="submit" type="submit" value="Register" class="btn btn-default" />
    </form>
</body>

</html>